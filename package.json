{
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": ".+spec.js$"
  },
  "name": "niketa",
  "displayName": "niketa",
  "description": "auto run jest tests with coverage",
  "version": "0.5.2",
  "license": "MIT",
  "publisher": "selfrefactor",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:niketa.start"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "niketa.start",
        "title": "Niketa"
      }
    ],
    "keybindings": [
      {
        "command": "niketa.start",
        "key": "ctrl+1",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "de": "commit",
    "test": "jest src --watch"
  },
  "devDependencies": {
    "commit-message": "https://github.com/selfrefactor/commit-message#3.0.1",
    "jest": "^23.6.0",
    "typescript": "^3.0.0",
    "vscode": "^1.1.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selfrefactor/niketa.git"
  },
  "dependencies": {
    "fastify": "^1.13.0",
    "rambdax": "^0.20.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "commitMessage": {
    "workInProgress": "",
    "support": [
      "prepublish",
      "refactor"
    ]
  }
}
