{
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": ".+spec.js$"
  },
  "name": "niketa",
  "displayName": "niketa",
  "description": "auto run jest tests with coverage",
  "version": "0.7.0",
  "license": "MIT",
  "publisher": "selfrefactor",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:niketa.start"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "niketa.start",
        "title": "Niketa"
      }
    ],
    "keybindings": [
      {
        "command": "niketa.start",
        "key": "ctrl+1",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Niketa",
      "properties": {
        "niketa.defaultMode": {
          "type": "string",
          "default": "NO_COVERAGE",
          "description": "Set default mode of Niketa"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest src --watch"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "typescript": "^3.1.0",
    "vscode": "^1.1.22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selfrefactor/niketa.git"
  },
  "dependencies": {
    "fastify": "^1.13.1",
    "rambdax": "^0.24.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  }
}
