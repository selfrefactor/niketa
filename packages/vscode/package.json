{
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": ".+spec.js$"
  },
  "icon": "logo.png",
  "name": "niketa",
  "displayName": "niketa",
  "description": "Auto run jest tests with coverage",
  "version": "2.20.1",
  "license": "MIT",
  "publisher": "selfrefactor",
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:niketa.start"
  ],
  "main": "./src/extension",
  "files": [
    "src"
  ],
  "contributes": {
    "commands": [
      {
        "command": "niketa.start",
        "title": "Niketa"
      }
    ],
    "keybindings": [
      {
        "command": "niketa.start",
        "key": "ctrl+1",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Niketa",
      "properties": {
        "niketa.DEFAULT_MODE": {
          "type": "string",
          "default": "WITH_COVERAGE",
          "description": "Set default mode of Niketa"
        },
        "niketa.DISABLE_LINT": {
          "type": "boolean",
          "default": false,
          "description": "Disable lint in niketa node part"
        },
        "niketa.PORT_0": {
          "type": "number",
          "default": 3011,
          "description": "VSCode in port"
        },
        "niketa.PORT_1": {
          "type": "number",
          "default": 3012,
          "description": "VSCode out port"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest src --watch"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "typescript": "^3.2.2",
    "vscode": "^1.1.36"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selfrefactor/niketa.git"
  },
  "dependencies": {
    "fastify": "^2.11.0",
    "rambdax": "3.5.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "commitMessage": {
    "workInProgress": ""
  }
}
