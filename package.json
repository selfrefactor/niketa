{
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": ".+spec.js$"
  },
  "icon": "logo.png",
  "name": "niketa",
  "displayName": "niketa",
  "description": "auto run jest tests with coverage",
  "version": "1.3.6",
  "license": "MIT",
  "publisher": "selfrefactor",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:niketa.start"
  ],
  "main": "./src/extension",
  "files": [
    "src"
  ],
  "contributes": {
    "commands": [
      {
        "command": "niketa.start",
        "title": "Niketa"
      }
    ],
    "keybindings": [
      {
        "command": "niketa.start",
        "key": "ctrl+1",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Niketa",
      "properties": {
        "niketa.DEFAULT_MODE": {
          "type": "string",
          "default": "WITH_COVERAGE",
          "description": "Set default mode of Niketa"
        },
        "niketa.PORT_0": {
          "type": "number",
          "default": 3011,
          "description": "VSCode in port"
        },
        "niketa.PORT_1": {
          "type": "number",
          "default": 3012,
          "description": "VSCode out port"
        },
        "niketa.PORT_2": {
          "type": "number",
          "default": 3013,
          "description": "Niketa notify port"
        },
        "niketa.PORT_3": {
          "type": "number",
          "default": 3014,
          "description": "Niketa notify close port"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest src --watch"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "typescript": "^3.2.2",
    "vscode": "^1.1.33"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selfrefactor/niketa.git"
  },
  "dependencies": {
    "fastify": "^2.3.0",
    "rambdax": "2.6.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "commitMessage": {
    "workInProgress": ""
  }
}
