  "author": "Dejan Toteff <deyan8284@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "fs-extra": "^9.0.0",
    "init-puppeteer": "4.3.6",
    "inquirer": "^7.0.0",
    "helpers-fn": "0.1.1",
    "rambdax": "^3.7.0"
  },
  "devDependencies": {
    "@types/fs-extra": "8.1.0",
    "@types/jest": "24.9.1",
    "@types/node": "12.12.31",
    "@types/puppeteer": "2.0.1",
    "env-fn": "^1.1.2",
    "jest": "25.2.3",
    "ts-jest": "25.3.1",
    "tslint-fn": "0.2.1",
    "typescript": "3.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
